    "axios": "^1.6.2",
    "kleur": "^4.1.5",
    "react": "^18.2.0",
    "react-cookie": "^6.1.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "ncp": "^2.0.0",
    "prettier": "^2.8.5",
    "rimraf": "^4.1.3"
